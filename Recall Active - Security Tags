SELECT 
    LP.TC_PURCHASE_ORDERS_ID AS PO,
    PTT.CNTR_NBR AS iLPN,
    LPNStatus.description AS LPN_STATUS,
    LH1.DSP_LOCN AS FROM_LOCN,
    LH2.DSP_LOCN AS TO_LOCN,
    
         SUBSTR(LH2.DSP_LOCN, 1, 3) AS Floor,
CASE
    WHEN SUBSTR(LH2.DSP_LOCN, 1, 3) IN ('AS1', 'AS2', 'AS3', 'PA1', 'PA2', 'PA3') THEN
        CONCAT(
            SUBSTR(LH2.DSP_LOCN, 3, 1),
            CASE
                WHEN TO_NUMBER(SUBSTR(LH2.DSP_LOCN, 8, 2)) <= 20 THEN '1'
                ELSE '3'
            END
        )
    ELSE NULL
END AS Zone,
    LH2.WORK_GRP,
    IC.ITEM_NAME,
    Current_SKU.ITEM_NAME AS CURRENT_LOCN_SKU,
    CASE WHEN IC.ITEM_NAME != Current_SKU.ITEM_NAME THEN 'New SKU Assigned'
    else null end as SKU_Status,
    IC.DESCRIPTION,
    PTT.NBR_UNITS as QTY,
    LD.CNTRY_OF_ORGN AS COO,
    PTT.MODULE_NAME,
    PTT.MENU_OPTN_NAME,
    PTT.CREATE_DATE_TIME + INTERVAL '3' HOUR as Transaction_Time,
    LD.BUSINESS_PARTNER_ID,
    LD.INVENTORY_TYPE
    FROM WM.PROD_TRKG_TRAN PTT
    JOIN WM.LOCN_HDR LH1 ON PTT.FROM_LOCN = LH1.LOCN_ID
    JOIN WM.LOCN_HDR LH2 ON PTT.TO_LOCN = LH2.LOCN_ID
    JOIN WM.LPN LP ON LP.TC_LPN_ID =  PTT.CNTR_NBR
    JOIN WM.ITEM_CBO IC ON PTT.ITEM_ID = IC.ITEM_ID
    JOIN WM.LPN_DETAIL LD ON LD.LPN_ID = LP.LPN_ID 
    JOIN wm.lpn_facility_status LPNStatus ON LP.lpn_facility_status = LPNStatus.lpn_facility_status
                            AND  LPNStatus.inbound_outbound_indicator = 'I'
     JOIN WM.wm_inventory wmi ON WMI.LOCATION_ID = LH2.LOCN_ID
      JOIN WM.ITEM_CBO Current_SKU ON WMI.ITEM_ID = Current_SKU.ITEM_ID
     
    WHERE LP.TC_PURCHASE_ORDERS_ID IN 
    ('18864974','18864986','18864992','18864998','18834345','18803190','18831598','18832035','18832423','18832447','18832745','18832818','18833020','18833045','18833543','18834428','18834294','18832910','18832911','18834220','18834319','18834360','18834370','18831281','18831292','18832314','18832364','18833298','18833865','18864978','18834001','18831335','18831384','18831621','18831650','18832234','18832293','18832851','18832998','18833103','18833190','18833236','18833866','18864949','18834043','18834109','18834143','18834167','18834191','18834221','18834249','18834285','18834295','18834320','18834371','18834393','18831610','18803228','18831664','18831694','18831787','18831828','18831992','18832083','18832129','18832557','18832999','18833522','18833544','18833628','18833642','18833671','18833714','18833759','18833816','18864955','18864961','18864969','18864975','18864982','18864987','18864993','18864999','18834429','18803210','18831365','18831366','18832483','18832499','18832620','18832639','18832640','18832677','18832678','18832692','18832693','18832713','18832714','18832765','18832766','18832835','18809850','18822442','18875036','18875048','18874392','18874410','18874424','18831581','18831689','18864946','18831706','18831720','18831740','18831853','18831914','18831934','18831459','18831753','18831875','18832520','18832888','18832961','18831460','18832889','18832962','18831754','18831993','18832084','18832315','18832694','18832852','18832912','18833104','18833299','18833949','18864988','18815751','18815785','18815821','18871515','18834487','18834503','18834581','18834648','18834658','18839807','18839815','18865008','18839834','18839844','18839871','18839924','18839958','18840050','18840076','18840120','18840142','18840158','18840202','18840239','18840261','18840335','18840423','18840459','18840815','18841956','18842077','18842102','18842129','18842189','18842212','18842223','18842281','18842337','18842368','18842394','18842426','18842448','18842481','18842492','188