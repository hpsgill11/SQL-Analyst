    AND MENU_OPTN_NAME LIKE ('Fill Actv - Truck')
    AND LH2.WORK_GRP = 'SHLF'
    and LPNStatus.description = 'Case consumed to active'
    ORDER BY PTT.CREATE_DATE_TIME DESC
