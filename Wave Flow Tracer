select
AID.TC_ORDER_ID,
AID.INVN_NEED_TYPE,
IC.ITEM_NAME,
IC.DESCRIPTION,
IC.PRODUCT_CLASS_ID,
IFMW.PICK_LOCN_ASSIGN_TYPE,
AID.ORIG_REQMT,
AID.QTY_ALLOC,
AID.QTY_PULLD,
--AID.PULL_LOCN_ID,
--AID.DEST_LOCN_ID,
AID.INVN_TYPE,
LH.LOCN_BRCD AS PULL_LOC,
PLH.PICK_LOCN_ASSIGN_ZONE AS PULL_LOC_PLAZ,
PLH.REPL_LOCN_BRCD,
LH2.LOCN_BRCD AS DEST_LOC,
PLH2.PICK_LOCN_ASSIGN_ZONE AS DEST_LOC_PLAZ,
LH2.PICK_DETRM_ZONE,
--PLH2.REPL_LOCN_BRCD,
AID.TASK_GENRTN_REF_NBR AS "WAVE#",
LH.AREA || LH.ZONE || LH.AISLE || LH.BAY AS "Area_Zone_Aisle_Bay"
, AID.ALLOC_INVN_DTL_ID
, LH2.LVL
, AID.CREATE_DATE_TIME
, LH2.LOCN_PICK_SEQ AS DEST_PICK_SEQ
, LH2.LOCN_PUTWY_SEQ
, LH2.LOCN_DYN_ASSGN_SEQ
from WM.ALLOC_INVN_DTL AID
LEFT JOIN WM.ORDERS O ON O.TC_ORDER_ID = AID.TC_ORDER_ID
LEFT JOIN WM.LOCN_HDR LH ON LH.LOCN_ID = AID.PULL_LOCN_ID
LEFT JOIN WM.LOCN_HDR LH2 ON LH2.LOCN_ID = AID.DEST_LOCN_ID
LEFT JOIN WM.WM_INVENTORY WI on WI.LOCATION_ID = AID.PULL_LOCN_ID
LEFT JOIN WM.ITEM_CBO IC on IC.ITEM_ID = AID.ITEM_ID
LEFT JOIN WM.ITEM_FACILITY_MAPPING_WMS IFMW ON IFMW.ITEM_ID = WI.ITEM_ID
LEFT JOIN WM.PICK_LOCN_HDR PLH ON PLH.LOCN_ID = AID.PULL_LOCN_ID
LEFT JOIN WM.PICK_LOCN_HDR PLH2 ON PLH2.LOCN_ID = AID.DEST_LOCN_ID
WHERE 

AID.TASK_GENRTN_REF_NBR IN ('202408200084')

--(#Prompt('pWave')#)

ORDER BY AID.PULL_LOCN_ID ASC
