SELECT C.MSG_ID,

       CQ.STATUS,

       C.EVENT_ID,

       C.WHEN_CREATED,

       C.DATA,

       CQ.WHEN_QUEUED,

       CQ.WHEN_STATUS_CHANGED

FROM CL_MESSAGE C,

     CL_ENDPOINT_QUEUE CQ

WHERE C.MSG_ID = CQ.MSG_ID

  AND TRUNC(CQ.WHEN_QUEUED) >= TRUNC(SYSDATE)

  AND C.DATA LIKE '%PW0111%'

ORDER BY C.WHEN_CREATED DESC ;
