SELECT
 --  item_cbo.item_id,
   item_cbo.item_name,
   item_cbo.item_style || '-' || item_cbo.item_color AS style_option,
   item_cbo.item_style,
   item_cbo.item_color,
   item_cbo.color_desc,
   item_cbo.item_size_desc,
   item_cbo.description,
   item_cbo.ref_field2,
   item_cbo.ref_field3,
   CASE
      WHEN
         vascomp_lpns.process_dates IS NOT NULL 
      THEN
         vascomp_lpns.process_dates 
      ELSE
         'Not Repriced' 
   END
   AS vascomp_date, 
   recalled_lpns.recall_task_desc,
   lpn.tc_purchase_orders_id, 
   lpn.tc_lpn_id, 	--lpn.TC_PARENT_LPN_ID AS PALLET,
   lpnstatus.description AS lpn_status,
   lpn_inventory_type.inventory_type,
   LPN_QTY.qty as Quantity,
   current_lpn_location.dsp_locn AS current_location,
    current_lpn_location.work_grp AS current_work_group,
   previous_lpn_location.dsp_locn AS previous_location,
   consumed_lpns_in_active.dsp_locn AS currently_in_active_inventory,
   immd_needs.cmnt as immediate_need_comment,
    lpn.rcvd_dttm,	
   lpn.last_updated_dttm
FROM
   item_cbo 
   LEFT JOIN
      lpn 
      ON lpn.item_id = item_cbo.item_id 
   LEFT JOIN
      lpn_facility_status lpnstatus 
      ON lpn.lpn_facility_status = lpnstatus.lpn_facility_status 
      AND lpnstatus.inbound_outbound_indicator = 'I' 
   LEFT JOIN
      locn_hdr current_lpn_location 
      ON current_lpn_location.locn_id = lpn.curr_sub_locn_id 
   LEFT JOIN
      locn_hdr previous_lpn_location 
      ON previous_lpn_location.locn_id = lpn.prev_sub_locn_id 
   LEFT JOIN
      (
         SELECT
            lpn_id,
            inventory_type 
         FROM
            lpn_detail
      )
      lpn_inventory_type 
      ON lpn_inventory_type.lpn_id = lpn.lpn_id 
   LEFT JOIN
      (
         SELECT
            p.cntr_nbr,
            listagg( DISTINCT to_char(p.create_date_time + INTERVAL '3' HOUR, 'MM/DD/YYYY'), ',' ) WITHIN GROUP ( 
         ORDER BY
            to_char(p.create_date_time + INTERVAL '3' HOUR, 'MM/DD/YYYY') ) AS process_dates,
            l.dsp_locn AS located_location 
         FROM
            prod_trkg_tran p 
            JOIN
               lpn 
               ON lpn.tc_lpn_id = p.cntr_nbr 
               AND lpn.inbound_outbound_indicator = 'I' 
            LEFT JOIN
               locn_hdr l 
               ON l.locn_id = p.to_locn 
         WHERE
            l.dsp_locn = 'VAS-SP-CO-MP' 
         GROUP BY
            p.cntr_nbr,
            l.dsp_locn
      )
      vascomp_lpns 
      ON vascomp_lpns.cntr_nbr = lpn.tc_lpn_id 
   LEFT JOIN
      (
         SELECT
            CASE
               WHEN
                  td.cntr_nbr IS NULL 
               THEN
                  td.carton_nbr 
               ELSE
                  td.cntr_nbr 
            END
            AS lpn, listagg(DISTINCT th.task_desc, ', ') WITHIN GROUP (
         ORDER BY
            th.task_desc) AS recall_task_desc 
         FROM
            task_dtl td 
            JOIN
               task_hdr th 
               ON td.task_id = th.task_id 
            LEFT JOIN
               locn_hdr destination_locn 
               ON td.dest_locn_id = destination_locn.locn_id 
         WHERE
            destination_locn.dsp_locn = 'VAS-SP-EC-PRJ' 
            AND th.invn_need_type = '92' 
            AND td.stat_code < 99 
         GROUP BY
            CASE
               WHEN
                  td.cntr_nbr IS NULL 
               THEN
                  td.carton_nbr 
               ELSE
                  td.cntr_nbr 
            END
      )
      recalled_lpns 
      ON recalled_lpns.lpn = lpn.tc_lpn_id 
   LEFT JOIN
      (
         SELECT
            cntr_nbr,
            dsp_locn,
            create_date_time 
         FROM
            (
               SELECT
                  p.cntr_nbr,
                  l.dsp_locn,
                  p.create_date_time,
                  ROW_NUMBER() OVER ( PARTITION BY l.dsp_locn 
               ORDER BY
                  p.create_date_time DESC ) AS rn 
               FROM
                  prod_trkg_tran p 
                  INNER JOIN
                     locn_hdr l 
                     ON l.locn_id = p.to_locn 
               WHERE
                  l.locn_class = 'A' 
                  AND 
                  (
                     p.menu_optn_name = 'Fill Actv User Dir' 
                     OR p.menu_optn_name = 'Fill Actv - Truck'
                  )
            )
         WHERE
            rn = 1
      )
      consumed_lpns_in_active 
      ON consumed_lpns_in_active.cntr_nbr = lpn.tc_lpn_id 
LEFT JOIN (Select distinct IMMD_NEEDS.po_nbr,
item_cbo.item_name,
IMMD_NEEDS.cmnt
from IMMD_NEEDS
inner join lpn on lpn.tc_asn_id = IMMD_NEEDS.tc_asn_id and lpn.tc_purchase_orders_id = IMMD_NEEDS.po_nbr
left join item_cbo on item_cbo.item_id = IMMD_NEEDS.item_id) immd_needs on immd_needs.po_nbr = lpn.tc_purchase_orders_id and immd_needs.item_name = item_cbo.item_name
left join (WITH qty_sums AS (
    SELECT lpn.tc_LPN_ID,
           NVL(SUM(ld.RECEIVED_QTY), 0) AS RECEIVED_QTY,
           NVL(SUM(ld.SHIPPED_QTY), 0)  AS SHIPPED_QTY,
           NVL(SUM(ld.INITIAL_QTY), 0)  AS INITIAL_QTY,
           NVL(SUM(ld.size_value), 0)   AS SIZE_VALUE,
           NVL(SUM(lt.nbr_units), 0)    AS LAST_TXN_QTY
    FROM wm.lpn_detail ld
    INNER JOIN wm.lpn lpn
           ON lpn.lpn_id = ld.lpn_id
          AND lpn.inbound_outbound_indicator = 'I'
    LEFT JOIN (
        SELECT cntr_nbr, nbr_units
        FROM (
            SELECT p.cntr_nbr,
                   p.nbr_units,
                   ROW_NUMBER() OVER (
                       PARTITION BY p.cntr_nbr
                       ORDER BY p.create_date_time DESC
                   ) rn
            FROM wm.prod_trkg_tran p
            WHERE p.menu_optn_name IN ('Fill Actv User Dir', 'Fill Actv - Truck')
        )
        WHERE rn = 1
    ) lt
      ON lt.cntr_nbr = lpn.tc_lpn_id
    GROUP BY lpn.tc_LPN_ID
)
SELECT tc_lpn_id,
       GREATEST(
         RECEIVED_QTY,
         SHIPPED_QTY,
         INITIAL_QTY,
         SIZE_VALUE,
         LAST_TXN_QTY
       ) AS qty
FROM qty_sums) LPN_QTY on LPN_QTY.tc_lpn_id = lpn.tc_lpn_id
WHERE
   1 = 1 
   and lpn.tc_lpn_id is not null
   AND lpn.lpn_facility_status != 90 	-- In Active Storage
   and lpn.lpn_facility_status != '99' --- Cancelled LPNs/Split Combines
   --- PHASE 2 STYLES BELOW
   AND 
   (
   item_cbo.item_style || '-' || item_cbo.item_color in ('LW6CLQS-0001','LW6CLQS-068585','LW6CLQS-031382','LW5FARS-045739','LW5FARS-0001','LW5FARS-028948','LW5FARS-019746','LW5FARS-068585','LW5FARS-030210','LW5FARS-031382','LW5FARS-070108','LW5FLOS-0001','LW5FLOS-019746','LW5FLOS-068585','LW5FLOS-031382','LW5FLOS-070108','LW5FARS-025461','LW5FMAS-0001','LW5FMAS-031382','LW5FLNS-0001','LW5FLNS-031382','LW6BKKS-0001','LW6BKKS-038452','LW6BKKS-019746','LW6BKKS-030210','LW6BKKS-069026','LW6BRSS-062468','LW6BKKS-049106','LW6BRSS-062220','LW6BKKS-031382','LW5DCES-045739','LW5DCES-0001','LW5EHBS-0001','LW5DCES-038452','LW5DCES-068585','LW5DCES-030210','LW5DSJS-047184','LW5DVXS-047184','LW5DCES-069026','LW5DSHS-025461','LW5DSHS-062468','LW5DCES-049106','LW5DSHS-062220','LW5DCES-031382','LW5EHBS-031382','LW5HPRS-031382','LW5DCES-019746','LW5HPRS-030210','LW5DQ1S-0001','LW5DQ1S-019746','LW5DQ1S-030210','LW5DQ1S-069026','LW5DQ1S-049106','LW5DQ1S-031382','LW5DQ3T-0001','LW5DQ3T-030210','LW5DQ3T-031382','LW5HG8S-0001','LW5HACS-0001','LW5HACS-068585','LW5HACS-035955','LW5HDPS-025461','LW5HACS-070108','LW5HACS-068578','LW5GP2S-031382','LW5FLNS-049106','LW5FARS-062214','LW5FARS-071168','LW5FARS-071188','LW5FARS-033190','LW6CEPS-045739','LW6CEPS-0001','LW6CK0S-047184','LW6CEPS-031382','LW5EPSS-045739','LW5EPSS-0001','LW5GJ4S-0001','LW5EPSS-068585','LW5EPSS-030210','LW5GJ4S-030210','LW5EXLS-069795','LW5EPSS-049106','LW5HOVS-049106','LW5EPSS-031382','LW5GJ4S-031382','LW5EPSS-019746','LW5FFFS-0001','LW5GBLS-0001','LW5HRBS-0001','LW5FFFS-019746','LW5FFFS-068585','LW5FFFS-030210','LW5FNOS-025461','LW5DCES-1179','LW5FFFS-062468','LW5FNOS-062220','LW5FFFS-031382','LW5FFFS-070108','LW5EPSS-034697','LW5EYKS-033068','LW5CVMS-0001','LW5CVMS-031382','LW5CVMS-070108','LW5HODS-0001','LW5HODS-031382','LW5CVMS-069026','LW5DFXA-031382','LW5HIMS-0001','LW5HIMS-031382','LW5HIMS-070108','LW5HILT-0001','LW5HILT-031382','LW5HILT-070108','LW5GNQT-031382','LW5GNUT-033454','LW5GNQT-070108','LW5GNQT-069026',